<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.saltyfish.backend.mapper.UserMapper">

    <select id="pageBeanHistoryByUserId" resultType="com.saltyfish.backend.pojo.vo.BeanHistoryVO">
